{
  "name": "gcut-cutting-planner",
  "version": "1.0.0.1",
  "homepage": "https://gfel.in/gcut",
  "author": {
    "name": "Gonzalo Melgarejo",
    "email": "gonzalo@gmdev.org",
    "url": "https://gfel.in/gcut"
  },
  "scripts": {
    "start": "vite",
    "build": "vite build",
    "build-stage": "env-cmd -f .env.qa vite build",
    "preview": "vite preview",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint:fix": "eslint --fix \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint:check": "eslint \"src/**/*.{js,jsx,ts,tsx}\" --max-warnings 0",
    "prettier": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "prettier:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format": "npm run prettier && npm run lint:fix",
    "format:check": "npm run prettier:check && npm run lint:check",
    "code-quality": "npm run format:check && npm run build",
    "knip": "knip"
  },
  "dependencies": {
    "@ant-design/colors": "8.0.0",
    "@ant-design/icons": "6.0.0",
    "@emotion/cache": "11.14.0",
    "@emotion/react": "11.14.0",
    "@emotion/styled": "^11.14.1",
    "@fontsource/inter": "5.2.5",
    "@fontsource/poppins": "5.2.5",
    "@fontsource/public-sans": "5.2.5",
    "@fontsource/roboto": "5.2.5",
    "@mui/base": "5.0.0-beta.70",
    "@mui/icons-material": "^7.1.2",
    "@mui/lab": "7.0.0-beta.14",
    "@mui/material": "7.1.2",
    "@mui/system": "7.1.1",
    "@mui/x-charts": "8.6.0",
    "@vitejs/plugin-react": "4.5.2",
    "d3": "^7.9.0",
    "formik": "2.4.6",
    "framer-motion": "12.8.2",
    "html2canvas": "^1.4.1",
    "i18next": "^25.2.1",
    "i18next-browser-languagedetector": "^8.2.0",
    "jspdf": "^3.0.1",
    "lodash-es": "4.17.21",
    "react": "19.1.0",
    "react-device-detect": "2.2.3",
    "react-dom": "19.1.0",
    "react-i18next": "^15.5.3",
    "react-number-format": "5.4.4",
    "react-router": "7.6.3",
    "react-router-dom": "7.6.3",
    "simplebar-react": "3.3.0",
    "swr": "2.3.3",
    "vite": "7.0.0",
    "web-vitals": "4.2.4",
    "vite-jsconfig-paths": "2.0.1",
    "yup": "1.6.1"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@eslint/compat": "1.2.8",
    "@eslint/eslintrc": "3.3.1",
    "@eslint/js": "9.25.1",
    "eslint": "9.25.1",
    "eslint-config-prettier": "10.1.2",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-prettier": "5.2.6",
    "eslint-plugin-react": "7.37.4",
    "eslint-plugin-react-hooks": "5.2.0",
    "immutable": "4.3.7",
    "knip": "5.45.0",
    "prettier": "3.5.3",
    "prettier-eslint-cli": "8.0.1"
  },
  "packageManager": "yarn@4.9.1"
}
